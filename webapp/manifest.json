{
	"_version": "1.1.0",
	"sap.app": {
		"id": "mind2mdepuswiss",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZMS_MDE_PU_SWISS_SRV": {
				"uri": "/sap/opu/odata/sap/ZMS_MDE_PU_SWISS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/ZMS_MDE_PU_SWISS_SRV/metadata.xml"
				}
			}
		},
		"_version": "1.3.0"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"_version": "1.3.0",
		"supportedThemes": []
	},
	"sap.ui5": {
		"flexEnabled": false,
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.f": {},
				"sap.suite.ui.generic.template": {},
				"sap.ui.comp": {},
				"sap.ui.generic.app": {},
				"sap.ui.table": {},
				"sap.ushell": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "mind2mdepuswiss.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZMS_MDE_PU_SWISS_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "mind2mdepuswiss.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "TargetWerksselect",
					"pattern": "",
					"target": [
						"TargetWerksselect"
					]
				},
				{
					"name": "ProdOverview",
					"pattern": "toProdOverview/:Path:",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"ProdOverview"
					]
				},
				{
					"name": "ProdDetail",
					"pattern": "toProdDetail/:Path:",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"ProdDetail"
					]
				}
			],
			"targets": {
				"TargetWerksselect": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "",
					"viewName": "Werksselect"
				},
				"ProdOverview": {
					"viewType": "XML",
					"viewName": "ProdOverview"
				},
				"ProdDetail": {
					"viewType": "XML",
					"viewName": "ProdDetail"
				}
			}
		},
		"rootView": {
			"viewName": "mind2mdepuswiss.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"_version": "1.2.0"
	}
}